# Editor configuration, see http://editorconfig.org
root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4
end_of_line = lf

[*.md]
trim_trailing_whitespace = false

[package.json]
indent_size = 2

[yarn.lock]
end_of_line = crlf
